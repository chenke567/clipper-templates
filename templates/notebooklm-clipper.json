			"type": "date"
		},
		{
			"name": "description",
			"value": "{{selectorHtml:.summary-content|strip_tags}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "{{\\\"5-10 tag keywords for Obsidian vault, 1-2 words each, kebab-case, comma-separated\\\"}},notebookLM",
