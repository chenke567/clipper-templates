			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings,Gemini",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://gemini.google.com/app/"
	],
	"noteNameFormat": "{{title|safe_name}}",
	"path": "Clippings/Chats/Gemini"
}